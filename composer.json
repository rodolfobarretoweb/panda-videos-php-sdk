  "minimum-stability" : "stable",
  "required": {
    "php": ">=7.1.0"
  },
  "authors": [
    {
      "name": "Rodolfo Ribeiro Barreto",
      "email": "rodolfobarretoweb@gmail.com"
    }
  ],
  "config" : {
    "vendor-dir" : "vendor",
    "bin-dir" : "bin"
  },
